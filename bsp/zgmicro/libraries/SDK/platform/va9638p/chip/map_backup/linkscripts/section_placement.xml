<!DOCTYPE Linker_Placement_File>
<Root name="Flash Section Placement">
  <MemorySegment name="ROM">
    <ProgramSection load="Yes" name=".vectors" />
    <ProgramSection alignment="4" load="Yes" name=".init" />
    <ProgramSection alignment="4" load="Yes" name=".init_rodata" />
    <ProgramSection alignment="4" load="Yes" runin=".text_run" name=".text" />
    <ProgramSection alignment="4" load="Yes" name=".dtors" />
    <ProgramSection alignment="4" load="Yes" name=".ctors" />
    <ProgramSection alignment="4" load="Yes" name=".rodata" />
    <ProgramSection alignment="4" load="Yes" name=".srodata" />
    <ProgramSection alignment="4" load="Yes" name=".eh_frame" keep="Yes" />
    <ProgramSection alignment="4" load="Yes" name=".gcc_except_table" />
    <ProgramSection alignment="4" load="Yes" runin=".fast_run" name=".fast" />
    <ProgramSection alignment="4" load="Yes" runin=".data_run" name=".data" />
    <ProgramSection alignment="4" load="Yes" runin=".tdata_run" name=".tdata" />
    <ProgramSection alignment="4" load="Yes" runin=".sdata_run" name=".sdata" inputsections="*(.sdata.* .sdata2.*)" />
    <ProgramSection alignment="4" load="Yes" runin=".roconstdata_run" name=".roconstdata" />
    <ProgramSection alignment="4" load="Yes" runin=".text_ram_run" name=".text_ram" />
	<ProgramSection alignment="4" load="Yes" name=".vdev_rom" />
  </MemorySegment>
  <MemorySegment name="RAM">
  	<!ProgramSection alignment="4" load="No" name=".rodata_run" />
    <ProgramSection alignment="4" load="No" name=".fast_run" />
    <ProgramSection alignment="4" load="No" name=".data_run" />
    <ProgramSection alignment="4" load="No" name=".tdata_run" />
    <ProgramSection alignment="4" load="No" name=".bss" />
    <ProgramSection alignment="4" load="No" name=".tbss" />
    <ProgramSection alignment="4" load="No" name=".sdata_run" />
    <ProgramSection alignment="4" load="No" name=".sbss" />
    <ProgramSection alignment="4" load="No" name=".non_init" />
    <ProgramSection alignment="4" size="__HEAPSIZE__" load="No" name=".heap" />
    <ProgramSection alignment="4" size="__STACKSIZE__" load="No" place_from_segment_end="Yes" name=".stack" />
    <ProgramSection alignment="4" load="No" name=".text_ram_run" />  
  </MemorySegment>
  <MemorySegment name="RAM_DEV">
	<ProgramSection alignment="4" load="No" name=".text_run" /> 
	<ProgramSection alignment="4" load="No" name=".vdev_ram" />
    <ProgramSection alignment="4" load="No" name=".roconstdata_run" />
  </MemorySegment>
  <MemorySegment name="RAM_SHARE">
  </MemorySegment>
</Root>
